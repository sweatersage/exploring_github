CREATE TABLE character_information (
  CharacterID int,
  FirstName varchar(50),
  Surname varchar(50),
  Age int,
  ExternalConflict varchar(255),
  InternalConflict varchar(255),
  BirthDate float,
  TarotCard varchar(50),
  MyerBriggs varchar(10),
  GodTier varchar(50),
  
);
